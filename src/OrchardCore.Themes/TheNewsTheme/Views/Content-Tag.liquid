{% zone "Header" %}
<div class="page-title-area">

    <div class="d-table">
        <div class="d-table-cell">
            <div class="container">
                <div class="page-title-content">
                    {% assign search = "Tag" | t %}
                    <h2>{% page_title search %}</h2>
                    <ul>
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li>
                            
                            <i class="fas fa-tag fa-xs fa-rotate-90 align-middle" aria-hidden="true"></i>
                            <span class="align-middle">{{ Model.ContentItem.DisplayText }}</span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
{% endzone %}

{{ Model.Content.ContentsMetadata | shape_render }}
<section class="blog-section section-padding">
   
    <div class="container">
        <div class="row">


            {{ Model.Content.TermPart | shape_render }}


        </div>
    </div>
</section>

