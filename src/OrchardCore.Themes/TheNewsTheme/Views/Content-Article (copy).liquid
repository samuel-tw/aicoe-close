{% zone "Header" %}

<div class="page-title-area">
    <div class="d-table">
        <div class="d-table-cell">
            <div class="container">
                <div class="page-title-content">

                    <h2>{{ Model.ContentItem.DisplayText }}</h2>
                    <ul>
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li>{{ Model.ContentItem.Content.Article.Subtitle.Text }}</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

{% endzone %}


{{ Model.Content.ContentsMetadata.Value }}
{{ Model.Content.ContentsMetadata | shape_render }}



<section class="privacy-policy ptb-100">
    <div class="some-class-name2">
        <button type="button" class="btn btn-warning zoomin">{{"放大字體"|t}}</button>
        <button type="button" class="btn btn-warning zoomout">{{"縮小字體"|t}}</button>
        <a class="jfontsize-button" id="jfontsize-m2" href="#">A-</a>
        <a class="jfontsize-button" id="jfontsize-d2" href="#">A</a>
        <a class="jfontsize-button" id="jfontsize-p2" href="#">A+</a>

        <hr />
        <div class="single-privacy some-class-name2">
            <div class="some-class-name2">

                {{ Model.Content.HtmlBodyPart | shape_render }}
            </div>


        </div>

    </div>

</section>




